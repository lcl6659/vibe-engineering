{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "rootDirectory": "frontend",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./frontend",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "regions": ["sin1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api-url"
  }
}
